{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Run wireguard",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "q",
                                    "modifiers": { "mandatory": ["control"] }
                                },
                                "to": [{ "shell_command": "osascript ~/dotfiles/scripts/wireguard.scpt" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Prevent command-m minimize window",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "m",
                                    "modifiers": { "mandatory": ["command"] }
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change command + h/j/k/l to Arrows",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": ["command"],
                                        "optional": ["caps_lock"]
